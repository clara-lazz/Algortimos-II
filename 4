#include <stdio.h>

// Ana Clara 02AM 424

int maior (int n1, int n2);

int main() {
    int n1, n2;

    printf("Insira dois valores inteiros:");
    scanf("%d %d", &n1, &n2);

    maior(n1,n2);
    
    return 0;
}

int maior(int n1, int n2){
    if (n1> n2)
    printf("%d é maior que %d",n1,n2);
    
    else
    printf("%d é maior que %d",n2,n1);
    
}
/* Relatório: Não tive nenhuma dificuldade nessa.*/
