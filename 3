#include <stdio.h>

// Ana Clara 02AM 424

int soma (int n1, int n2);

int main() {
    int n1, n2;

    printf("Insira dois valores inteiros:");
    scanf("%d %d", &n1, &n2);

    int r = soma(n1,n2);
    printf("%d", r);
    
    return 0;
}

int soma(int n1, int n2){
    return n1 + n2;
    
}
