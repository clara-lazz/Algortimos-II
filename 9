#include <stdio.h>

int multiplicarValores(int vetor[], int tamanho) {
    int produto = 1;  
    for (int i = 0; i < tamanho; i++) {
        produto *= vetor[i];  
    }
    return produto;
}

int main() {
    int tamanho;

    printf("Digite o tamanho do vetor: ");
    scanf("%d", &tamanho);

    int vetor[tamanho];

    printf("Digite os %d valores do vetor:\n", tamanho);
    for (int i = 0; i < tamanho; i++) {
        printf("Digite o valor %d: ", i + 1);
        scanf("%d", &vetor[i]);
    }

    int resultado = multiplicarValores(vetor, tamanho);

    printf("Resultado da multiplicação dos valores: %d\n", resultado);

    return 0;
}

